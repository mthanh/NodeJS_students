<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>SINHVIEN - LIST</title>
</head>
<body>
    Danh sach sinh vien
    <table width="500" border="1">
        <tr>
            <td>ID</td>
            <td>Hoten</td>
            <td>Email</td>
            <td><a href="../sinhvien/them">Them</a></td>
        </tr>

        <% danhsach.rows.forEach(function(sv){ %>
        <tr>
            <td><%= sv.id %></td>
            <td><%= sv.hoten %></td>
            <td><%= sv.email %></td>
            <td>
                <a href="../sinhvien/sua/<%= sv.id %>">Sua</a> - 
                <a onClick="return confirm('Are you sure?')" href="../sinhvien/xoa/<%= sv.id %>">Xoa</a>
            </td>
        </tr>
        <% }) %>
        
    </table>
</body>
</html>